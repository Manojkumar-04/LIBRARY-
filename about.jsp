  <script>
const teamCardsContainer = document.querySelector('.team-cards-container');

function scrollTeamCards() {
    teamCardsContainer.scrollLeft += 2; // Adjust the scrolling speed (increase or decrease the value as needed)

    // Check if the cards have reached the end and reset the scroll position
    if (teamCardsContainer.scrollLeft >= (teamCardsContainer.scrollWidth - teamCardsContainer.clientWidth)) {
        teamCardsContainer.scrollLeft = 0;
    }
}

// Set the interval for scrolling (every 10 milliseconds in this example, adjust as needed)
setInterval(scrollTeamCards, 10);


